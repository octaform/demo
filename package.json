{
  "name": "octaform-demo",
  "version": "1.0.5",
  "description": "Octaform Validate - Demo Page",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack-dev-server --config ./webpack/config.dev.js",
    "build:prod": "npm run clean && webpack -p --config ./webpack/config.prod.js",
    "build:dev": "npm run clean && webpack --config ./webpack/config.dev.js",
    "clean": "rimraf dist",
    "webpack": "node_modules/.bin/webpack",
    "rimraf": "node_modules/.bin/rimraf",
    "pkg:deploy": "npm run build:prod && gh-pages -b master --dotfiles --message 'Webpage Deploy' --dist dist/",
    "pkg:core:link": "yalc link octaform && cd .yalc/octaform && yarn install && cd ../..",
    "pkg:additional:link": "yalc link octaform-additional && cd .yalc/octaform-additional && yarn install && cd ../.."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octaform/octaform.github.io.git"
  },
  "author": "Octaform",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/octaform/octaform/issues"
  },
  "homepage": "https://github.com/octaform/octaform#readme",
  "dependencies": {
    "classnames": "^2.2.6",
    "octaform": "^1.4.2",
    "octaform-additional": "^1.1.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "circular-dependency-plugin": "^4.4.0",
    "css-loader": "^2.1.1",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "gh-pages": "^2.1.1",
    "git-revision-webpack-plugin": "^3.0.3",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "node-sass": "^4.7.2",
    "regenerator-runtime": "^0.11.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.2",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.1.4"
  },
  "optionalDependencies": {
    "yalc": "^1.0.0-pre.34"
  }
}
